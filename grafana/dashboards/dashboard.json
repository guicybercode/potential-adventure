{
  "dashboard": {
    "title": "Real-Time Processor Dashboard",
    "panels": [
      {
        "title": "Messages Processed",
        "targets": [
          {
            "expr": "sum(rate(realtime_processor_messages_total[5m]))"
          }
        ]
      },
      {
        "title": "Anomalies Detected",
        "targets": [
          {
            "expr": "sum(rate(realtime_processor_anomalies_total[5m]))"
          }
        ]
      },
      {
        "title": "Batch Processing Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(realtime_processor_batch_duration_microseconds_bucket[5m]))"
          }
        ]
      }
    ]
  }
}

